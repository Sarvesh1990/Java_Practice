public class SymbolTable {
	LinkedList[] myArray = new LinkedList[5];
	public void insert(String abc)
	{
		int i= (abc.hashCode() & 0x7fffffff)% myArray.length;
		System.out.println("Hash code is " + i);
		if(myArray[i] == null)
		{
			LinkedList node = new LinkedList();
			node.value=abc;
			myArray[i]=node;
		}
		else
		{
			LinkedList head=myArray[i];
			while(head.next!=null)
			{
				head=head.next;
			}
			System.out.println("Key for last linked list is " + head.value);
			LinkedList node = new LinkedList();
			node.value=abc;
			head.next=node;
		}
	}
	
	public void print()
	{
		for(int j=0; j<myArray.length; j++)
		{
			System.out.println("Array is " + myArray[j]);
		}
	}
}
