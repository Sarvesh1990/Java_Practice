import java.util.HashSet;
import java.util.Map;
import java.util.Set;

public class Graph 
{
	Map<Integer, Set<Integer> > myMap;
	void Insert(int a, int b)
	{
		if(myMap.get(a)==null)
		{
			Set <Integer> mySet= new HashSet<Integer>();
			mySet.add(b);			
		}
		else
		{
			myMap.get(a).add(b);
		}
	}
}
